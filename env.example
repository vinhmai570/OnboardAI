# Database Configuration (for local development connecting to Docker)
DATABASE_NAME=onboard_ai_development
TEST_DATABASE_NAME=onboard_ai_test
DATABASE_USERNAME=postgres
DATABASE_PASSWORD=password
DATABASE_HOST=localhost
DATABASE_PORT=5433

# Rails Configuration
RAILS_ENV=development
RAILS_MAX_THREADS=5

# pgAdmin Configuration (for docker-compose)
PGADMIN_EMAIL=admin@onboardai.com
PGADMIN_PASSWORD=admin

# Azure OpenAI Configuration (for embeddings)
AZURE_OPENAI_ENDPOINT=https://your-resource-name.openai.azure.com/
AZURE_OPENAI_EMBEDDINGS_DEPLOYMENT=text-embedding-3-small
OPENAI_API_KEY=your-openai-api-key-here

# Add your other environment variables here
# SECRET_KEY_BASE=your_secret_key_base
